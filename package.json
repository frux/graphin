{
  "name": "graphin",
  "version": "0.0.2",
  "description": "Isomorphic GraphQl client",
  "main": "dist/index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "build": "babel src/index.js -o dist/index.js && babel src/index.js -o dist/index.min.js --compact true --no-comments"
  },
  "keywords": [
    "graphql",
    "isomorphic",
    "client"
  ],
  "author": "frux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frux/graphin.git"
  },
  "bugs": {
    "url": "https://github.com/frux/graphin/issues"
  },
  "homepage": "https://github.com/frux/graphin#readme",
  "ava": {
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "fetch-ponyfill": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.11.6",
    "nyc": "^7.1.0",
    "xo": "^0.16.0"
  }
}
