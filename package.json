{
  "name": "graphin",
  "version": "0.0.11",
  "description": "Isomorphic GraphQl client",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && npm run ava",
    "lint": "xo",
    "ava": "ava",
    "nyc": "nyc ava",
    "build": "babel src/index.js -o dist/index.js && babel src/index.js -o dist/index.min.js --compact true --no-comments"
  },
  "keywords": [
    "graphql",
    "isomorphic",
    "client"
  ],
  "author": "frux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frux/graphin.git"
  },
  "bugs": {
    "url": "https://github.com/frux/graphin/issues"
  },
  "homepage": "https://github.com/frux/graphin#readme",
  "ava": {
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "fetch-ponyfill": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "nyc": "^8.1.0",
    "xo": "^0.16.0"
  }
}
